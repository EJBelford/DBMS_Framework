/*                                                                            */
/*                                 Populate                                   */
/*                                                                            */
/*--*----|----*----|----*----|----*----|----*----|----*----|----*----|----*---*/ 

DECLARE

    CURSOR code_cur IS
        SELECT a.xx_ID, a.xx_DESCRIPTION
        FROM pfsa_pba_appl_ref a
        ORDER BY a.catcode;
        
    code_rec    code_cur%ROWTYPE;
        
BEGIN 

    Insert into PFSA_PBA_APPL_REF
       (REC_ID, PBA_MENU_ID, PBA_MENU_DESC, PBA_MENU_SORT_ORDER, STATUS, UPDT_BY, LST_UPDT, ACTIVE_FLAG, ACTIVE_DATE, INACTIVE_DATE, INSERT_BY, INSERT_DATE, UPDATE_DATE, DELETE_FLAG, DELETE_DATE, HIDDEN_FLAG, HIDDEN_DATE)
    Values
       (1000, 'PBA1', 'FUTURE COMBAT SUPPORT/LOGISTICS SUPPORT ACTIVITY', 1, 'C', 'RDR00008', TO_DATE('05/06/2008 07:36:51', 'MM/DD/YYYY HH24:MI:SS'), 'Y', TO_DATE('01/01/1900 00:00:00', 'MM/DD/YYYY HH24:MI:SS'), TO_DATE('12/31/2099 00:00:00', 'MM/DD/YYYY HH24:MI:SS'), 'RDR00008', TO_DATE('05/06/2008 07:36:51', 'MM/DD/YYYY HH24:MI:SS'), TO_DATE('01/01/1900 00:00:00', 'MM/DD/YYYY HH24:MI:SS'), 'N', TO_DATE('01/01/1900 00:00:00', 'MM/DD/YYYY HH24:MI:SS'), 'Y', TO_DATE('01/01/1900 00:00:00', 'MM/DD/YYYY HH24:MI:SS'));
    
    COMMIT; 
    
    DBMS_OUTPUT.ENABLE(1000000);
    
    DBMS_OUTPUT.NEW_LINE;
    
    OPEN code_cur;
    
    LOOP
        FETCH code_cur 
        INTO  code_rec;
        
        EXIT WHEN code_cur%NOTFOUND;
        
        DBMS_OUTPUT.PUT_LINE(code_rec.xx_ID || ', ' || code_rec.xx_CODE 
            || ', ' || code_rec.xx_DESCRIPTION
            );
        
    END LOOP;
    
    CLOSE code_cur;
    
COMMIT;    

END;  
    
